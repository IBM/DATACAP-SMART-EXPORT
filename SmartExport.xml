<?xml version="1.0" encoding="UTF-8"?>
<SmartExport version="1.0">
  <header>/* Output generated by DataCap custom action */</header>
  <body>
    <statement name="statement1">
      <rules>
        <rule name="rule1">
          <condition>[DCO.Flight.Air_Ticket.Total_Cost] > 800 and [DCO.Flight.Air_Ticket.Airfare] > 600</condition>
          <value>[DCO.Flight.Air_Ticket.Return_From] </value>
        </rule>
        <default>[DCO.Flight.Air_Ticket.Outbound_From]</default>
      </rules>
      <output>From station = [value]</output>
    </statement>
    <statement name="statement2">
      <rules>
        <rule name="rule1">
          <condition>[DCO.Flight.Air_Ticket.Total_Cost] > 800 and [DCO.Flight.Air_Ticket.Airfare] > 600</condition>
          <value>[DCO.Flight.Air_Ticket.Return_To] </value>
        </rule>
        <default>[DCO.Flight.Air_Ticket.Outbound_To]</default>
      </rules>
      <output>To station = [value]</output>
    </statement>
    <statement name="statement3"> 
      <output>Airfare = [DCO.Flight.Air_Ticket.Airfare]</output>
    </statement>
  </body>
  <footer>Total Cost = [DCO.Flight.Air_Ticket.Total_Cost]</footer>
</SmartExport>